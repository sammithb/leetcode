# O(n) time and O(1) space solution 

class Solution:
    def removeDuplicates(self, nums: List[int]) -> int:
        
        final_element = nums[0]
        k = 1 
            
        for i in range (0, len(nums)):
            if nums[i] != final_element: 
                nums[k] = nums[i]
            
                k += 1 
            
                final_element = nums[i]
        
        return k 
    